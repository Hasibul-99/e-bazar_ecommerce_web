div className=\"product-bottom-details\">\r\n                        <div className=\"product-price\">\r\n                            {\r\n                                product.discountPrice ? <Fragment>\r\n                                    <small>à§³{product.sellPrice}</small>\r\n                                    à§³{ product.sellPrice - product.discountPrice }\r\n                                </Fragment> : <Fragment>à§³{product.sellPrice}</Fragment> \r\n                            }\r\n                        </div>\r\n                        <div className=\"product-links\">\r\n                            <span className=\"cursore-pointer mr-2\" onClick={()=> openModal(product._id)}>\r\n                                <i className=\"fa fa-eye\"></i>\r\n                            </span>\r\n                            <span className=\"cursore-pointer\" onClick={() => addProductIncard(product)}>\r\n                                <i className=\"fa fa-shopping-cart\"></i>\r\n                            </span>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            \r\n            <ProductModalView\r\n                productId={product._id} \r\n                isOpen={isOpen}\r\n                product={product}\r\n                update={update}\r\n                HandelModalClose={handelModalClose}>\r\n            </ProductModalView>\r\n\r\n            <br/>\r\n            <br/>\r\n            <br/>\r\n        </Fragment>\r\n        \r\n    )\r\n}\r\n","E:\\react\\e-bazar_ecommerce_web\\src\\scripts\\api-service.js",[],"E:\\react\\e-bazar_ecommerce_web\\src\\scripts\\helper.js",["335","336","337"],"E:\\react\\e-bazar_ecommerce_web\\src\\pages\\Public\\Landing\\NonBrandItems.js",["338"],"import React, {useEffect, useState, Fragment} from 'react';\r\nimport { Link } from \"react-router-dom\";\r\nimport { getData } from \"../../../scripts/api-service\";\r\nimport { GET_CATEGORY_LIST } fr
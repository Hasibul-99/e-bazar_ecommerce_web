es = await getData(GET_RPODUCT+ '?_id=' + productId);\r\n\r\n        if (res?.data?.isSuccess) {\r\n            let data = res?.data?.data[0];\r\n\r\n            setProduct(data);\r\n            setSelected(data.photos[0]);\r\n        }\r\n    }\r\n\r\n    const changeProduct = (item) => {\r\n        setSelected(item);\r\n    }\r\n\r\n    const handelQuantuty = ({qun, productId}) => {\r\n        setQuan(qun);\r\n    }\r\n\r\n    const addToCard = () => {\r\n        findCardProduct(productId).then(res => {\r\n            if (res) {\r\n                updateQuamtity(productId, quan);\r\n            } else {\r\n                addNewProduct(product, quan);\r\n            }\r\n        });\r\n    }\r\n\r\n\r\n    return (\r\n        <div className=\"home-landing tips-info\">\r\n            <div className=\"non-Brand-items card\">\r\n                <div className=\"card-body\">\r\n                    {product ? (\r\n                        <div className=\"row\">\r\n                            <div className=\"col-sm-12 col-md-6\">\r\n                                <div className=\"selected-image\">\r\n                                    <GlassMagnifier\r\n                                        imageSrc={`http://easyexpress24.com:5000/static/${selected}`}\r\n                                        imageAlt=\"Example\"\r\n                                        largeImageSrc={`http://easyexpress24.com:5000/static/${selected}`}\r\n                                        magnifierSize=\"50%\"\r\n                                        allowOverflow=\"true\"\r\n                                        zoom=\"200%\"\r\n                                    />\r\n                                </div>\r\n                                    \r\n                                <div className=\"d-flex mt-4\">\r\n                                    { \r\n\r\n                                        product?.photos.map(item => {\r\n                                            return <